{
  "name": "frontend-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": "16.16.0"
  },
  "packageManager": "npm@8.19.2",
  "dependencies": {
    "classnames": "^2.3.2",
    "jest": "^29.1.2",
    "next-transpile-modules": "^9.0.0",
    "react-hot-toast": "^2.4.0",
    "usehooks-ts": "^2.9.1"
  }
}
